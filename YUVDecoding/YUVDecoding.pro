TEMPLATE = app
CONFIG += c++11 console

FFMPEGPATH = E:/FFMPEG/ffmpeg-dev
INCLUDEPATH += $${FFMPEGPATH}/include
DEPENDPATH += $${FFMPEGPATH}/include

LIBS += -L$${FFMPEGPATH}/lib -lavcodec \
    -lavdevice \
    -lavfilter \
    -lavformat \
    -lavutil \
    -lpostproc \
    -lswresample \
    -lswscale
SOURCES += main.cpp
